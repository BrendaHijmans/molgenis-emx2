{
  "query": {
    "select": [
      "firstName",
      "lastName",
      {
        "father": [
          "firstName"
        ]
      }
    ],
    "filter": {
      "lastName": {
        "eq": [
          "Duck"
        ]
      }
      "father": {
        "firstName": {
          "eq": "Donald"
        }
      }
    }
  }
}

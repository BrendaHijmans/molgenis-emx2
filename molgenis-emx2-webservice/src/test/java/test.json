{
  "query": {
    "select": [
      "firstName",
      "lastName",
      "father/firstName"
    ],
    "filter": {
      "lastName": {
        "eq": "Duck"
      },
      "Age": {
        "eq": 50
      }
      "father": {
        "firstName": {
          "eq": "Donald"
        }
      }
    },
    "sort": {
      "FirstName": "asc",
      "LastName": "desc"
    }
  }
}
